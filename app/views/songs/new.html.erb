<h1>Create a Song</h1>

<%= form_for @song  do |f| %>

  <%= f.label :title %><br>
  <%= f.text_field :title %><br>
  <%= f.label :artist %><br>
  <%= f.text_field :artist, id: "song_artist_name" %><br>
  <%= f.collection_select :genre_id, Genre.all, :id, :name %><br>
  <%= f.text_field :notes, name: "song[song_notes][]", id: "song_notes_1", list: "artist_list" %>
  <%= f.text_field :notes, name: "song[song_notes][]", id: "song_notes_2", list: "artist_list" %>

  <%= f.submit %>

<% end %>


